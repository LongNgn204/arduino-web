{
    "compilerOptions": {
        "target": "ES2021",
        "lib": [
            "ES2021"
        ],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "strict": true,
        "skipLibCheck": true,
        "noEmit": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "types": [
            "@cloudflare/workers-types"
        ],
        "paths": {
            "@/*": [
                "./src/*"
            ]
        },
        "baseUrl": "."
    },
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}